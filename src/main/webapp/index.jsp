  <section id="show_case" name="show_case">
    <div id="showcase">
      <div class="container">
        <div class="row">
          <h1 class="centered">Inscription</h1>
          <br>
          <div class="col-lg-6 centered">
          <form method="post" action="HelloServlet" class="form">
            <fieldset class="fieldset">

              <legend class="legend">Inscription</legend>
              <p>Vous pouvez vous inscrire via ce formulaire.</p>

              <label class="label"  for="email">Adresse email <span class="requis">*</span></label>
              <input class="input" type="text" id="email" name="email" value="" size="20" maxlength="60" />
              <span class="erreur">${erreur['email']}</span>
              <br />

              <label  class="label" for="prenom">Prenom <span class="requis">*</span></label>
              <input class="input" type="text" id="prenom" name="prenom" value="" size="20" maxlength="60" />
              <span class="erreur">${erreur['prenom']}</span>
              <br />

              <label  class="label" for="nom">Nom <span class="requis">*</span></label>
              <input class="input" type="text" id="nom" name="nom" value="" size="20" maxlength="60" />
              <span class="erreur">${erreur['nom']}</span>
              <br />

              <label  class="label" for="password">Mot de passe <span class="requis">*</span></label>
              <input class="input" type="password" id="password" name="password" value="" size="20" maxlength="20" />
              <span class="erreur">${erreur['password']}</span>
              <br />

              <label  class="label" for="confirmation">Confirmation du mot de passe <span class="requis">*</span></label>
              <input class="input" type="password" id="confirmation" name="confirmation" value="" size="20" maxlength="20" />
              <br />


              <input class="input" type="submit" value="Inscription" name="bouton" />
              <br />

              <p class="${empty erreur ? 'succes' : 'erreur'}">${resultat}</p>
            </fieldset>
          </form>
          </div>
          <!-- Connexion-->
          <div class="col-lg-6 centered">
          <form method="post" action="HelloServlet" class="form">
            <fieldset class="fieldset">
